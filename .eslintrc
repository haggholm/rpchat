{
  "env": {
    "node": true
  },
  "settings": {
    "ecmascript": 6
  },
  "ecmaFeatures": {
    "arrowFunctions": true,
    "binaryLiterals": true,
    "blockBindings": true,
    "classes": true,
    "defaultParams": true,
    "destructuring": true,
    "forOf": true,
    "generators": true,
    "modules": true,
    "objectLiteralComputedProperties": true,
    "objectLiteralDuplicateProperties": true,
    "objectLiteralShorthandMethods": true,
    "objectLiteralShorthandProperties": true,
    "octalLiterals": true,
    "regexUFlag": true,
    "regexYFlag": true,
    "restParams": true,
    "spread": true,
    "superInFunctions": true,
    "templateStrings": true,
    "unicodeCodePointEscapes": true,
    "globalReturn": false,
    "ecmascript": 6,
    "jsx": true,
    "experimentalObjectRestSpread": false
  },
  "rules": {
    "no-cond-assign": 1,
    "no-console": 0,
    "no-debugger": 1,
    "no-dupe-args": 2,
    "no-dupe-keys": 2,
    "no-duplicate-case": 2,
    "no-empty-character-class": 1,
    "no-extra-boolean-cast": 1,
    "no-extra-semi": 1,
    "no-func-assign": 2,
    "no-inner-declarations": 2,
    "no-invalid-regexp": 2,
    "no-irregular-whitespace": 1,
    "no-negated-in-lhs": 2,
    "no-obj-calls": 1,
    "no-regex-spaces": 0,
    "no-sparse-arrays": 1,
    "no-unreachable": 1,
    "use-isnan": 2,
    "valid-jsdoc": 1,
    "valid-typeof": 2,
    "no-unexpected-multiline": 1,
    "curly": [1, "multi-line"],
    "no-alert": 1,
    "no-caller": 2,
    "no-eq-null": 1,
    "no-eval": 2,
    "no-implicit-coercion": [1, { "boolean": false, "string": false, "number": true}],
    "no-implied-eval": 2,
    "no-iterator": 2,
    "no-lone-blocks": 2,
    "no-multi-spaces": 1,
    "no-multi-str": 2,
    "no-native-reassign": 2,
    "no-new-func": 2,
    "no-new-wrappers": 2,
    "no-octal-escape": 2,
    "no-octal": 2,
    "no-proto": 2,
    "no-redeclare": 1,
    "no-return-assign": 1,
    "no-script-url": 2,
    "no-self-compare": 1,
    "no-sequences": 1,
    "no-throw-literal": 1,
    "no-unused-expressions": 1,
    "no-useless-call": 1,
    "no-warning-comments": [1, { "terms": [ "todo", "fixme", "xxx"], "location": "start"}],
    "no-with": 2,
    "radix": 1,
    "no-delete-var": 2,
    "no-label-var": 2,
    "no-shadow-restricted-names": 2,
    "no-undef": 2,
    "no-unused-vars": 1,
    "no-use-before-define": [2, "nofunc"],
    "brace-style": [2, "1tbs", { "allowSingleLine": true}],
    "camelcase": [1, { "properties": "never"}],
    "comma-spacing": [1, { "before": false, "after": true}],
    "comma-style": [2, "last"],
    "consistent-this": [2, "self"],
    "eol-last": 2,
    "indent": [2, 2],
    "new-cap": 1,
    "no-array-constructor": 2,
    "no-mixed-spaces-and-tabs": 2,
    "no-spaced-func": 1,
    "no-unneeded-ternary": 1,
    "quotes": [1, "single", "avoid-escape"],
    "semi-spacing": 1,
    "semi": [2, "always"],
    "space-infix-ops": 0,
    "space-return-throw-case": 2,
    "space-unary-ops": 2,
    "spaced-comment": 1,
    "arrow-parens": 1,
    "arrow-spacing": 1,
    "generator-star-spacing": 1,
    "no-class-assign": 2,
    "no-const-assign": 2,
    "no-dupe-class-members": 2,
    "prefer-const": 0,
    "prefer-spread": 1,
    "prefer-template": 1,
    "require-yield": 2,
    "no-bitwise": 1,
    "max-len": [1, 200]
  }
}
